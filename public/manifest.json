{
    "name": "Sunrun Salesforce Extension",
    "description": "Tool for salesforce info",
    "version": "1.4",
    "manifest_version": 3,
    "icons": {
        "128": "icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
     "storage", "tabs", "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}